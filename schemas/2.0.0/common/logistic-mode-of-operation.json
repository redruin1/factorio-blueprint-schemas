            "const": 0
        },
        {
            "title": "Set Requests",
            "description": "`defines.control_behavior.cargo_landing_pad.exclusive_mode.set_requests`",
            "const": 1
        },
        {
            "title": "None",
            "description": "`defines.control_behavior.cargo_landing_pad.exclusive_mode.none`",
            "const": 2
        }
    ],
    "default": 0
}
