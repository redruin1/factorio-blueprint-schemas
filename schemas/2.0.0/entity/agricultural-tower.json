{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "$id": "urn:factorio:entity:agricultural-tower",
    "title": "Agricultural Tower",
    "description": "An entity that seeds and harvests plants automatically.",
    "allOf": [
        {"$ref": "urn:factorio:entity"},
        {"$ref": "urn:factorio:common:control-behavior"},
        {"$ref": "urn:factorio:common:circuit-enabled"},
        {"$ref": "urn:factorio:common:logistic-condition"}
    ],
    "type": "object",
    "properties": {
        "control_behavior": {
            "properties": {
                "read_contents": {
                    "description": "Whether or not this entity should broadcast the contents of its inventory to a connected circuit network.",
                    "type": "boolean",
                    "default": false
                }
            }
        }
    }
}